---
layout: post.njk
---

{% block content %}
  <div class=" f4 lh-copy">
    {{ content | safe }}
  </div>

  <div class="flex flex-col space-y-10">
    {%- for tweet in collections[collection] | reverse -%}
      {% set path = tweet.url %}
      {% set text = tweet.templateContent %}
      {% include "tweetinc.njk" %}
    {%- endfor -%}
  </div>
{% endblock %}
